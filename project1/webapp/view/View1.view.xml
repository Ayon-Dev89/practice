		<f:SimpleForm id="form1"
			editable="false"
			layout="ColumnLayout"
			columnsM="2"
			columnsL="3"
			columnsXL="4"
			>
			<f:content>
				 
				<Label id="label1" text="ProductID" />
				<Text id="text1" text="{FormModel>/ProductID}" />
				<Label id="label2" text="Name" />
				<Text id="text2" text="{FormModel>/Name}"/>
				<Label id="label3" text="SupplierName" />
				<Text id="text3" text="{FormModel>/SupplierName}" />
				<Label id="label4" text="Price" />
				<Text id="text4" text="{FormModel>/Price}" />
				<Label id="label5" text="City" />
				<Text id="text5" text="{FormModel>/ToSupplier/Address/City}" />
				<Label id="label6" text="PostalCode" />
				<Text id="text6" text="{FormModel>/ToSupplier/Address/PostalCode}" />
				<Label id="label7" text="Street" />
				<Text id="text7" text="{FormModel>/ToSupplier/Address/Street}" />
				<Label id="label8" text="Country" />
				<Text id="text8" text="{FormModel>/ToSupplier/Address/Country}" />
				<Label id="label9" text="CompanyName" />
				<Text id="text9" text="{FormModel>/ToSupplier/CompanyName}" />
				<Label id="label10" text="Email" />
				<Text id="text10" text="{FormModel>/ToSupplier/EmailAddress}" />
				<Label id="label11" text="PhoneNumber" />
				<Text id="text11" text="{FormModel>/ToSupplier/PhoneNumber}" />
				<Label id="label12" text="CurrencyCode" />
				<Text id="text12" text="{FormModel>/ToSupplier/CurrencyCode}" />
			</f:content>
			
		</f:SimpleForm>
	

	
    	<footer>
